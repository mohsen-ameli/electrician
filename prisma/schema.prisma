generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

enum articleType {
  residential
  commercial
  blog
}

model article {
  slug      String   @id
  createdAt DateTime @default(now())
  title     String
  content   String
  image     String?
  type      articleType
}

model admin {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username  String   @unique
  password  String
}
